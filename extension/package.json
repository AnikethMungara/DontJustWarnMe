{
  "name": "dontjustwarnme",
  "displayName": "Don'tJustWarnMe",
  "description": "Fix beginner Python code errors using AI.",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.70.0"
  },
  "categories": ["Other"],
  "activationEvents": [
    "onCommand:dontjustwarnme.fixCode"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "dontjustwarnme.fixCode",
        "title": "Don'tJustWarnMe: Fix Selected Python Code"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./"
  },
  "devDependencies": {
    "@types/vscode": "^1.70.0",
    "typescript": "^4.9.5"
  },
  "dependencies": {
    "node-fetch": "^3.3.2"
  }
}
